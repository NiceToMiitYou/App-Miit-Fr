<div ng-controller="resourcesInnerController" class="content ressources-inner-content" ng-class="{active : activetool === 'RESSOURCE-INNER'}">

    <div class="header navbar navbar-inverse ">

        <div class="navbar-inner">
          <a class="btn btn-primary previous-ressource" ng-click="track('RESSOURCE')" href="#ressources-inner"><i class="fa fa-chevron-left"></i></a>
            <div class="header-quick-nav">
                <div class="pull-left">
                    <h3><i class="fa fa-folder-open"></i> {{ current.name }}</h3>
                </div>
            </div>
        </div>
    </div>

    <div class="resources"
         ng-repeat="resource in current.resources"
         ng-click="openResource( resource )">
        
        <span>
            {{ resource.name }}
        </span>

    </div>

</div>